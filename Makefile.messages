all: update

create:
	menhir --list-errors --no-stdlib --infer src/syntax/parser.mly > haxe.messages

update:
	cp haxe.messages haxe.messages.backup
	menhir --update-errors haxe.messages.backup --no-stdlib --infer src/syntax/parser.mly > haxe.messages
	menhir --compile-errors haxe.messages --no-stdlib --infer src/syntax/parser.mly > src/syntax/syntaxErrors.ml